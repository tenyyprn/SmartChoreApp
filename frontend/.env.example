# Development environment variables (Local/Mock mode)
# Copy this file to .env.local and fill in your actual values

# Development Mode (使用できるクラウドサービスがない場合)
VITE_DEVELOPMENT_MODE=true
VITE_USE_MOCK_SERVICES=false

# Firebase Configuration (オプション - 利用可能な場合のみ)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=compact-haiku-454409-j0.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=compact-haiku-454409-j0
VITE_FIREBASE_STORAGE_BUCKET=compact-haiku-454409-j0.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Google Cloud Configuration (AI機能用)
VITE_GOOGLE_CLOUD_PROJECT_ID=compact-haiku-454409-j0
VITE_VERTEX_AI_LOCATION=us-central1
VITE_GOOGLE_ACCESS_TOKEN=your_access_token

# Google Calendar API (オプション)
VITE_GOOGLE_CALENDAR_API_KEY=your_calendar_api_key

# Application Configuration
VITE_APP_NAME=Smart Chore App
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# AI Demo Mode (ハッカソン用)
VITE_DEMO_MODE=true
VITE_AI_MOCK_MODE=true
